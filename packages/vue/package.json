    "@storybook/vue3": "^10.1.11",
    "@storybook/vue3-vite": "^10.1.11",
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.11",
    "storybook": "^10.1.11",
    "tailwindcss": "4.1.18",
