{
  "name": "@orion-ui/vue",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun run storybook",
    "build": "bun run --bun vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest --run",
    "lint": "bun run eslint .",
    "lint:fix": "bun run eslint . --fix",
    "type-check": "bun run vue-tsc",
    "storybook": "storybook dev -p 6006"
  },
  "peerDependencies": {
    "vue": ">=3.5.0"
  },
  "dependencies": {
    "@ark-ui/vue": "^5.30.0",
    "@orion-ui/theme": "workspace:*"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/blocks": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@storybook/vue3": "^10.1.11",
    "@storybook/vue3-vite": "^10.1.11",
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.11",
    "storybook": "^10.1.11",
    "tailwindcss": "4.1.18",
    "typescript": "^5.7.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  }
}
